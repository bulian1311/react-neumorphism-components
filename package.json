{
  "name": "react-neumorphism-components",
  "version": "0.1.1",
  "description": "React neumorphism component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "types": "dist/typings/index.d.ts",
  "check-types": "tsc",
  "test": "jest",
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup -c rollup.config.js",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "npm run test:ts && npm run test:jest",
    "test:jest": "jest --config jest.config.js",
    "test:ts": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write \"src/**/*\"",
    "lint": "npm run lint:format && npm run lint:js",
    "lint:format": "prettier --check  \"src/**/*\"",
    "lint:js": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bulian1311/react-neumorphism.git"
  },
  "keywords": [
    "react",
    "neumorphism",
    "css",
    "modal",
    "layout",
    "responsive",
    "bootstrap"
  ],
  "author": "bulian1311",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bulian1311/react-neumorphism/issues"
  },
  "homepage": "https://github.com/bulian1311/react-neumorphism#readme",
  "peerDependencies": {
    "prop-types": ">=15.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.4",
    "nanoid": "^3.1.30",
    "postcss": "^8.4.4",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "styled-components": "^5.3.3"
  }
}
